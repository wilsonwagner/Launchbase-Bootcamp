{% extends "layout.njk" %}
{% block head %}

<title>Projetos - Wilson Wagner</title>
<link href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet">

{% endblock %}

{% block content %}

<div id="TITULO">
    <h1>CONTEÃšDOS</h1>
</div>

<section class="grids">
    {% for item in items %}
    <div class="grid" id="{{item.id}}">
        <div class="gridImagem">
            <img src="{{item.icon}}" alt="Imagem do {{item.id}}">
        </div>
        <div class="gridConteudo">
            <p>{{item.id}}</p>
        </div>
        <div class="gridInfo">
            <P>{{item.modules}}</P>
            <p class="gridPrice">{{item.price}}</p>
        </div>
    </div>
    {% endfor %}
</section>
{% endblock %}